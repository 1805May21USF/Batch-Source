CREATE TABLE BANKUSER (
USERID INT PRIMARY KEY,
FIRSTNAME VARCHAR2(25),
LASTNAME VARCHAR2(25),
EMAIL VARCHAR2(50),
USERNAME VARCHAR2(25),
PASSWORD VARCHAR2(25));

CREATE TABLE CLIENT (
CLIENTID INT PRIMARY KEY,
USERID INT);

CREATE TABLE BANKEMPLOYEE (
EMPLOYEEID INT PRIMARY KEY,
USERID INT);

CREATE TABLE ADMIN (
ADMINID INT PRIMARY KEY,
USERID INT);

CREATE TABLE ACCOUNT (
ACCOUNTID INT PRIMARY KEY,
CLIENTID INT,
BALANCE NUMBER);

CREATE SEQUENCE USERIDSEQ
MINVALUE 1
MAXVALUE 1000
INCREMENT BY 1;

CREATE SEQUENCE EMPLOYEEIDSEQ
MINVALUE 1
MAXVALUE 1000
INCREMENT BY 1;

CREATE SEQUENCE CLIENTIDSEQ
MINVALUE 1
MAXVALUE 1000
INCREMENT BY 1;

CREATE SEQUENCE ADMINIDSEQ
MINVALUE 1
MAXVALUE 1000
INCREMENT BY 1;

CREATE SEQUENCE ACCOUNTIDSEQ
MINVALUE 1
MAXVALUE 1000
INCREMENT BY 1;

